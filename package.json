{
  "name": "shamir",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^12.8.3",
    "bcp-47-match": "^1.0.3",
    "bcp-47-normalize": "^1.1.1",
    "jquery": "^3.6.0",
    "js-crypto-aes": "^1.0.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-intl": "^5.13.4",
    "react-scripts": "4.0.3",
    "scrypt-js": "^3.0.1",
    "secrets.js-grempe": "^2.0.0",
    "web-vitals": "^1.1.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "extract": "formatjs extract",
    "compile": "formatjs compile",
    "easy-extract": "formatjs extract 'src/**/*.js*' --out-file lang/en.json --id-interpolation-pattern '[sha512:contenthash:base64:6]'",
    "easy-compile": "formatjs compile lang/en.json --ast --out-file src/compiled-lang/en.json"
  },
  "eslintConfig": {
    "plugins": [
      "formatjs"
    ],
    "rules": {
      "formatjs/no-offset": "error",
      "formatjs/enforce-description": [
        "error",
        "literal"
      ],
      "formatjs/enforce-default-message": [
        "error",
        "literal"
      ],
      "formatjs/enforce-placeholders": [
        "error",
        {
          "ignoreList": []
        }
      ],
      "formatjs/no-camel-case": "error",
      "formatjs/no-multiple-whitespaces": "error",
      "formatjs/no-multiple-plurals": "error",
      "formatjs/enforce-id": "error"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@formatjs/cli": "^3.2.0",
    "babel-plugin-formatjs": "^9.2.5",
    "eslint-plugin-formatjs": "^2.12.8"
  }
}
